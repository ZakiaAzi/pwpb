        </activity>
        <activity
            android:name=".Profil"
            android:label="Profil" >
            <intent-filter>
                <action android:name="com.example.activity_profil" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Informasi"
            android:label="Informasi" >
            <intent-filter>
                <action android:name="com.example.activity_informasi" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Bantuan"
            android:label="Bantuan" >
            <intent-filter>
                <action android:name="com.example.activity_bantuan" />

                <category android:name="android.intent.category.DEFAULT" />
